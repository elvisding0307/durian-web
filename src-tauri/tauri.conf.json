{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Durian",
  "version": "0.1.0",
  "identifier": "com.durian-web.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "笃里安",
        "width": 1200,
        "height": 750,
        "minWidth": 1200,
        "minHeight": 750
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "publisher": "ElvisDing",
    "copyright": "Copyright © 2025 ElvisDing. All rights reserved.",
    "category": "Productivity",
    "shortDescription": "密码管理器",
    "longDescription": "笃里安是一款安全可靠的密码管理器，可以帮助用户安全地存储和管理各种账户密码。",
    "license": "Apache",
    "licenseFile": "../LICENSE"
  }
}
